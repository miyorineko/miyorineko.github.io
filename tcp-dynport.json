{
  "stats": {},
  "api": {
    "services": [
      "StatsService",
      "HandlerService"
    ],
    "tag": "api"
  },
  "policy": {
    "system": {
      "statsUserDownlink": true,
      "statsUserUplink": true
    }
  },
  "inbound": {
    "port": 24110,
    "protocol": "vmess",
    "settings": {
      "clients": [
        {
          "id": "UserUUID",
          "alterId": 100
        }
      ],
      "disableInsecureEncryption": true
    },
    "streamSettings": {
      "network": "tcp"
    },
    "detour": {
      "to": "dynport"
    }
  },
  "outbound": {
    "protocol": "freedom",
    "settings": {}
  },
  "inboundDetour": [
    {
      "listen": "127.0.0.1",
      "port": 10010,
      "protocol": "dokodemo-door",
      "settings": {
        "address": "127.0.0.1"
      },
      "tag": "api"
    },
    {
      "protocol": "vmess",
      "port": "24111-30000",
      "tag": "dynport",
      "settings": {},
      "allocate": {
        "strategy": "random",
        "concurrency": 5,
        "refresh": 5
      },
      "streamSettings": {
        "network": "tcp"
      }
    }
  ],
  "outboundDetour": [
    {
      "protocol": "blackhole",
      "settings": {},
      "tag": "blocked"
    },
    {
      "protocol": "freedom",
      "settings": {
        "redirect": "miyorineko.github.io:80"
      },
      "tag": "restrict"
    }
  ],
  "routing": {
    "strategy": "rules",
    "settings": {
      "rules": [
        {
          "type": "field",
          "ip": [
            "0.0.0.0/8",
            "10.0.0.0/8",
            "100.64.0.0/10",
            "127.0.0.0/8",
            "169.254.0.0/16",
            "172.16.0.0/12",
            "192.0.0.0/24",
            "192.0.2.0/24",
            "192.168.0.0/16",
            "198.18.0.0/15",
            "198.51.100.0/24",
            "203.0.113.0/24",
            "::1/128",
            "fc00::/7",
            "fe80::/10"
          ],
          "outboundTag": "blocked"
        },
        {
          "type": "chinaip",
          "outboundTag": "restrict"
        },
        {
          "type": "chinasites",
          "outboundTag": "restrict"
        },
        {
          "type": "field",
          "inboundTag": [
            "api"
          ],
          "outboundTag": "api"
        }
      ]
    }
  }
}
